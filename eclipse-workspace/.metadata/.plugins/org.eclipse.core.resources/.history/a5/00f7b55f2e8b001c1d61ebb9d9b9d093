package beans;

public class Product {
	
	private String desc;
	private float price; 

	public Product(String Desc, float Price) {
		this.desc = Desc; 
		this.price = Price; 
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public float getPrice() {
		return price;
	}

	public void setPrice(float price) {
		this.price = price;
	}
	

}
