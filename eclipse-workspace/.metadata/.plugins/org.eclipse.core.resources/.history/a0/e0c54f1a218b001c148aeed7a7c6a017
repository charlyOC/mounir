	public static void main(String[] args) {
		
		FileWriter writer = null;
		try {
			writer = new FileWriter("db.txt");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

		try(PrintWriter printWriter =
		    new PrintWriter(writer)){

		    printWriter.write("burger ");
		    printWriter.println(6); 
		    printWriter.write("pizza ");
		    printWriter.println(9);
		    printWriter.write("kebab ");
		    printWriter.print(6.50);
		    
		    
		    BufferedReader reader = new BufferedReader(new FileReader("db.txt"));
		    String currentLine = reader.readLine();
		    reader.close();

		    assertEquals(expected_value, currentLine);
