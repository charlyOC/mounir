package beans;

public class Product {
	
	private String desc;
	private int price; 
	private int quantity;

	public Product(String Desc, int Price, int quantity) {
		this.desc = Desc; 
		this.price = Price; 
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	

}
