package main;


import java.io.File;
import java.io.PrintWriter;

public class Main {


	public static void main(String[] args) {
		
	    PrintWriter writer2 = null;
	    writer2 = new PrintWriter(new File("C:\\file.txt"));
	    writer2.write("Hello, Welcome to WayToLearnX!");
	    writer2.flush();
	    writer2.close();	
		
	}

}
